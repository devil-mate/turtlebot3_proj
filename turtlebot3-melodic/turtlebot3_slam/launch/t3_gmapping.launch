<launch>
  <!-- Arguments -->
  <!-- <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/> -->
  <arg name="model" default="burger" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="configuration_basename" default="turtlebot3_lds_2d.lua"/>
  <arg name="set_base_frame" default="base_footprint"/>
  <arg name="set_odom_frame" default="odom"/>
  <arg name="set_map_frame"  default="map"/>

  <!-- Gmapping -->
  <node pkg="gmapping" type="slam_gmapping" name="t3_slam_gmapping" output="screen">
      <rosparam file="$(find turtlebot3_slam)/config/t3_gmapping_params.yaml" command="load"/>
  </node>
  <!-- 改进的gmapping -->
    <!-- <node pkg="amend_gmapping" type="slam_gmapping" name='gmapping_node' output='screen'>
      <rosparam file="$(find turtlebot3_slam)/config/t3_gmapping_params.yaml" command="load"/>
  </node> -->
</launch>
